<?xml version="1.0" encoding="utf-8"?>
<Project>
  <PropertyGroup>
    <PackageJsonName Condition="$(PackageJsonName) == ''">ws4kp-streamer</PackageJsonName>
    <PackageJsonVersion Condition="$(PackageJsonVersion) == ''">1.0.0</PackageJsonVersion>
    <PackageJsonDescription Condition="$(PackageJsonDescription) == ''">Streams ws4kp from Puppeteer to FFmpeg as HLS.</PackageJsonDescription>
    <PackageJsonMain Condition="$(PackageJsonMain) == ''">streamer.mjs</PackageJsonMain>
    <PackageJsonType Condition="$(PackageJsonType) == ''">module</PackageJsonType>
    <PackageJsonScriptsStart Condition="$(PackageJsonScriptsStart) == ''">concurrently "node streamer.mjs" "node server.mjs"</PackageJsonScriptsStart>
    <PackageJsonAuthor Condition="$(PackageJsonAuthor) == ''"></PackageJsonAuthor>
    <PackageJsonLicense Condition="$(PackageJsonLicense) == ''">ISC</PackageJsonLicense>
    <PackageJsonDependenciesConcurrently Condition="$(PackageJsonDependenciesConcurrently) == ''">^8.2.2</PackageJsonDependenciesConcurrently>
    <PackageJsonDependenciesExpress Condition="$(PackageJsonDependenciesExpress) == ''">^4.19.2</PackageJsonDependenciesExpress>
    <PackageJsonDependenciesFfmpegStatic Condition="$(PackageJsonDependenciesFfmpegStatic) == ''">^5.2.0</PackageJsonDependenciesFfmpegStatic>
    <PackageJsonDependenciesPuppeteer Condition="$(PackageJsonDependenciesPuppeteer) == ''">^22.10.0</PackageJsonDependenciesPuppeteer>
  </PropertyGroup>
</Project>